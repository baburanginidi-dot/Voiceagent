{
  "name": "maya---nxtwave-voice-agent",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 5000",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:push:prod": "DATABASE_URL=$DATABASE_URL drizzle-kit push",
    "start": "npm run db:push:prod && concurrently \"vite preview --host 0.0.0.0 --port 5000\" \"tsx server/index.ts\""
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@neondatabase/serverless": "^1.0.2",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
